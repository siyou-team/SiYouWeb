                                <text>{{item.activity_name}}</text>
                            </view>
                        </scroll-view>
                    </view>
                    <view class="m-coupon-box-bottom">

                    </view>
                    <view class="u-close-btn" catchtap="outertouch">✕</view>
                </view>
            </view>-->
        </view>
    </view>
    <view class="m-tips" wx:if="{{!IsChannel}}">
        <view class="m-tips-logo">
            <image mode="aspectFit" src="../../assets/logo_tech.png"></image>
        </view>
        <view class="m-tips-str">商城套件系统提供技术支持 {{versionNumber}}</view>
    </view>
</view>

<view class="u-cleanbox"></view>
<view class="m-footer-btn" wx:if="{{ProductInfo.activityType!='GROUPBOOKING'}}">

    <block wx:if="{{ProductInfo.is_virtual}}">
        <view class="m-footer-btn-list">
            <view class="m-footer-btn-item" style="width:50%" data-id="{{ProductInfo.item_id}}">
                <view class="m-footer-btn-icon">
                    <contact-button size="27" type="default-dark" session-from="weapp">
                    </contact-button>
                    <label class="iconfont icon-more "></label>
                </view>
                <text>客服</text>
            </view>
            <view class="m-footer-btn-item" style="width:50%"  bindtap="PDCollection" data-id="{{ProductInfo.item_id}}">
                <view class="m-footer-btn-icon">
                    <label class="iconfont {{isCollection?'icon-favoritesfilling':'icon-favorite'}} {{isCollection?'highlight':''}}"></label>
                </view>
                <text>{{isCollection?'已收藏':'收藏'}}</text>
            </view>
        </view>
        <block wx:if="{{ProductInfo.Stock<=0}}">
            <view class="m-footer-btn-main m-footer-desable">
                立即预约
            </view>
            <view class="m-footer-btn-main m-footer-desable">
                库存不足
            </view>
        </block>
        <block wx:if="{{ProductInfo.Status=='offline'||ProductInfo.Pstatus=='offline'||ProductInfo.IsDelete}}">
            <view class="m-footer-btn-main m-footer-desable">
                立即预约
            </view>
            <view class="m-footer-btn-main m-footer-desable">
                商品已下架
            </view>
        </block>
        <block wx:else>
            <view wx:if="{{ProductInfo.SpecLst.length>0}}" class="m-footer-btn-main m-now" bindtap="ckselectsp1"
                  style="width:50%">
                立即预约
            </view>
            <view wx:else class="m-footer-btn-main m-now" catchtap="buynow" style="width:50%">
                立即预约
            </view>
            <!--<view wx:if="{{ProductInfo.SpecLst.length>0}}" class="m-footer-btn-main" bindtap="ckselectsp">
              加入购物车
            </view>
            <view wx:else class="m-footer-btn-main" catchtap="addCard">
              加入购物车
            </view>-->
        </block>
    </block>
    <block wx:else>
        <view class="m-footer-btn-list">
            <view class="m-footer-btn-item" data-id="{{proId}}">
                <view class="m-footer-btn-icon">
                    <contact-button size="27" type="default-dark" session-from="weapp">
                    </contact-button>
                    <label class="iconfont icon-more "></label>
                </view>
                <text>客服</text>
            </view>
            <view class="m-footer-btn-item" bindtap="PDCollection" data-id="{{ProductInfo.item_id}}">
                <view class="m-footer-btn-icon">
                    <label class="iconfont {{isCollection?'icon-favoritesfilling':'icon-star_off'}}  {{isCollection?'highlight':''}}"></label>
                </view>
                <text>{{isCollection?'已收藏':'收藏'}}</text>
            </view>
            <view data-url="../../pages/cart/cart" class="m-footer-btn-item" bindtap="goTabBar">
                <view class="m-footer-btn-icon">
                    <label class="iconfont icon-cart "></label>
                </view>
                <text>购物车</text>
            </view>
        </view>
        <block wx:if="{{ProductInfo.Stock<=0}}">
            <view class="m-footer-btn-main m-footer-desable">
                立即购买
            </view>
            <view class="m-footer-btn-main m-footer-desable">
                库存不足
            </view>
        </block>
        <block wx:if="{{ProductInfo.Status=='offline'||ProductInfo.Pstatus=='offline'||ProductInfo.IsDelete}}">
            <view class="m-footer-btn-main m-footer-desable">
                立即购买
            </view>
            <view class="m-footer-btn-main m-footer-desable">
                商品已下架
            </view>
        </block>
        <block wx:else>
            <view class="m-footer-btn-main" bindtap="ckselectsp">
                加入购物车
            </view>
            <!-- <view wx:else class="m-footer-btn-main" catchtap="addCard">
              加入购物车
            </view> -->
            <view wx:if="{{ProductInfo.SpecLst.length>0}}" class="m-footer-btn-main m-now" bindtap="ckselectsp1">
                立即购买
            </view>
            <view wx:else class="m-footer-btn-main m-now" catchtap="buynow">
                立即购买
            </view>
        </block>
    </block>
</view>
<view class="m-footer-btn m-fg-btn" wx:if="{{ProductInfo.activityType=='GROUPBOOKING'}}">
    <view class="m-footer-btn-list">
        <view class="m-footer-btn-item" data-id="{{proId}}" style="width:50%">
            <view class="m-footer-btn-icon">
                <contact-button size="27" type="default-dark" session-from="weapp">
                </contact-button>
                <label class="iconfont icon-more "></label>
            </view>
            <text>客服</text>
        </view>
        <view class="m-footer-btn-item" bindtap="PDCollection" data-id="{{proId}}" style="width:50%">
            <view class="m-footer-btn-icon">
                <label class="iconfont {{isCollection?'icon-favoritesfilling':'icon-favorite'}}  {{isCollection?'highlight':''}}"></label>
            </view>
            <text>{{isCollection?'已收藏':'收藏'}}</text>
        </view>
    </view>
    <block wx:if="{{ProductInfo.Stock<=0||ProductInfo.Status=='offline'||ProductInfo.Pstatus=='offline'||ProductInfo.IsDelete}}">
        <view class="m-footer-btn-main m-footer-desable u-fg">
            <view>¥{{ProductInfo.ItemSalePrice}}</view>
            <view>普通购买</view>
        </view>
        <view class="m-footer-btn-main m-footer-desable u-fg">
            <view>¥{{ProductInfo.ShowPrice}}</view>
            <view wx:if="{{!is_join}}">立即开团</view>
            <view wx:else>立即参团</view>
        </view>
    </block>
    <block wx:else>
        <!-- <view class="m-footer-btn-main m-now u-fg" bindtap="ILObuynow">
          <view>¥{{ProductInfo.ItemSalePrice}}</view>
          <view>普通购买</view>
        </view> -->
        <view class="m-footer-btn-main m-now u-fg" bindtap="ckselectsp" data-type="1">
            <view>¥{{ProductInfo.ItemSalePrice}}</view>
            <view>普通购买</view>
        </view>
        <view wx:if="{{ProductInfo.SpecLst.length>0}}" class="m-footer-btn-main u-fg" bindtap="ckselectsp">
            <view>¥{{ProductInfo.ShowPrice}}</view>
            <view wx:if="{{!is_join}}">立即开团</view>
            <view wx:else>立即参团</view>
        </view>
        <view wx:else class="m-footer-btn-main u-fg" catchtap="immediatelyOffered">
            <view>¥{{ProductInfo.ShowPrice}}</view>
            <view wx:if="{{!is_join}}">立即开团</view>
            <view wx:else>立即参团</view>
        </view>
    </block>
</view>
<view class="m-panel-sp {{selectsp==0?'hide':''}} " bindtap="closesp">
    <view class="m-panel-sp-content {{selectct==0?'bounceOutDown animated':'bounceInUp animated'}}" catchtap>
        <view class="m-panel-sp-pinfo">
            <view class="m-panel-sp-pimg">
                <image src="{{selectimg||ProductInfo.productPicList[0].Path}}"/>
            </view>
            <view class="m-panel-sp-pname">
                <view class="m-pprice" wx:if="{{ProductInfo.activityType=='GROUPBOOKING'&&isModified}}">
                    ¥{{ProductInfo.ItemSalePrice}}
                </view>
                <view class="m-pprice" wx:if="{{ProductInfo.activityType!='GROUPBOOKING'}}">¥{{ProductInfo.ShowPrice}}</view>
                <view class="m-pprice" wx:if="{{ProductInfo.activityType=='GROUPBOOKING'&&!isModified}}">
                    ¥{{ProductInfo.ShowPrice}}
                </view>
                <view class="m-pstock">库存{{ProductInfo.Stock}}件</view>
                <view wx:if="{{ProductInfo.SpecLst.length}}" class="m-pstock">已选择：
                    <block wx:for="{{ProductInfo.SpecLst}}" wx:for-item="items">
                        <block wx:for="{{items.svLst}}" wx:for-item="item">
                            <block wx:if="{{item.IsChecked}}">{{item.Name}}</block>
                        </block>
                    </block>
                </view>
            </view>
            <icon type="cancel" class="m-panel-sp-icon" color="#888" catchtap="closesp"/>
        </view>
        <scroll-view class="m-panel-sp-listbox" scroll-y="true">
            <block wx:for="{{ProductInfo.SpecLst}}" wx:for-item="items" wx:key="key1">
                <view class="m-panel-sp-listbox-item">
                    <view class="m-cells-title">{{items.Name}}</view>
                    <view class="m-panel-sp-labellist">
                        <block wx:for="{{items.svLst}}" wx:for-item="item">
                            <label wx:if="{{item.IsChecked}}" class="m-panel-sp-sellabel">{{item.Name}}</label>
                            <label wx:else data-ckid="{{items.ckid}}" data-unit_id="{{items.unit_id}}"
                                   data-spid="{{item.Id}}" catchtap="selectsp">{{item.Name}}
                            </label>
                        </block>
                    </view>
                </view>
            </block>
            <!-- <view style="height:96rpx;width:100%;"></view> -->
        </scroll-view>
        <view style="height:100rpx"></view>
        <view class="m-m-panel-sp-btn">
            <view class="m-m-panel-sp-lbtn"
                  style="{{ProductInfo.activityType=='GROUPBOOKING'&&isModified==false?'display:none':''}}">
                <view class="u-cart-num" style="margin-top:21rpx">
                    <view class="u-num-btn" catchtap="sub">-</view>
                    <input type="number" value="{{numval}}" bindblur="writenum"/>
                    <view class="u-num-btn" catchtap="add">+</view>
                </view>
            </view>
            <view class="m-m-panel-sp-rbtn" style="width:100%" catchtap="immediatelyOffered" wx:if="{{ProductInfo.activityType=='GROUPBOOKING'&&isModified==false&&!is_join}}">
                立即开团
            </view>
            <view class="m-m-panel-sp-rbtn" style="width:100%" catchtap="immediatelyOffered" wx:if="{{ProductInfo.activityType=='GROUPBOOKING'&&isModified==false&&is_join}}">
                立即参团
            </view>


            <block  wx:if="{{ProductInfo.is_virtual}}">
                <view class="m-m-panel-sp-rbtn" catchtap="buynow" wx:if="{{flag&&ProductInfo.activityType!='GROUPBOOKING'}}">
                    立即预约
                </view>
            </block>
            <block wx:else>
                <view class="m-m-panel-sp-rbtn" catchtap="addCard" wx:if="{{flag&&ProductInfo.activityType!='GROUPBOOKING'}}">
                    加入购物车
                </view>
            </block>
            <view class="m-m-panel-sp-rbtn" catchtap="buynow" wx:if="{{flag1&&ProductInfo.activityType!='GROUPBOOKING'}}">
                <label  wx:if="{{ProductInfo.is_virtual}}">立即预约</label>
                <label  wx:else>立即购买</label>
            </view>
            <view class="m-m-panel-sp-rbtn" catchtap="ILObuynow"
                  wx:if="{{ProductInfo.activityType=='GROUPBOOKING'&&isModified}}">立即购买
            </view>
        </view>
    </view>
</view>
</block>
<view class="m-nullpage" wx:else>
<view class="m-nullpage-middle">
    <label class="iconfont icon-meiyougengduo"></label>
    <view class="m-null-tip">
        <text>什么都没有</text>
    </view>
</view>
</view>

